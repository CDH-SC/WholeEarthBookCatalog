{
  "entrypoint": "index.html",
  "shell": "src/dhc-main.html",
  "fragments": [
    "src/dhc-help.html",
    "src/dhc-home.html",
    "src/dhc-create-account.html",
    "src/dhc-user-account.html",
    "src/dhc-book-profile.html",
    "src/dhc-book-results.html",
    "src/dhc-404.html",
    "src/frag-footer.html",
    "src/frag-header.html",
    "src/frag-rotation.html",
    "src/data-login.html",
    "src/data-search.html",
    "src/crypto-import.html",
    "src/frag-advanced-search.html"
  ],
  "sources": [
    "src/**/*",
    "images/*",
    "index.html",
    "src/dhc-main.html",
    "src/dhc-help.html",
    "src/dhc-home.html",
    "src/dhc-create-account.html",
    "src/dhc-user-account.html",
    "src/dhc-book-profile.html",
    "src/dhc-book-results.html",
    "src/dhc-404.html",
    "src/frag-footer.html",
    "src/frag-header.html",
    "src/frag-rotation.html",
    "src/data-login.html",
    "src/data-search.html",
    "src/crypto-import.html",
    "src/frag-advanced-search.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es5-bundled"
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-bundled"
    },
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  }
}