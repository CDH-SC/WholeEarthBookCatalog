    "shell": "src/dhc-main.html",
    "fragments": [
        "src/dhc-help.html",
        "src/dhc-home.html",
        "src/dhc-create-account.html",
        "src/dhc-user-account.html",
        "src/dhc-book-profile.html",
        "src/dhc-book-results.html",
        "src/dhc-404.html",
        "src/frag-footer.html",
        "src/frag-header.html",
        "src/frag-rotation.html",
        "src/data-login.html",
        "src/data-search.html",
        "src/crypto-import.html",
        "src/frag-advanced-search.html"
